generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  category    String
  techStack   String // Comma separated values
  imageUrl    String
  createdAt   DateTime @default(now())
}
